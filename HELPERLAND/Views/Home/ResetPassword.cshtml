@model HELPERLAND.Models.ViewModels.SignupViewmodel

<form method="post">
    <section class="facqheader">
        <p>Contact Us</p>
    </section>
    <div class="faHeaderDecoration">
        <span class="line"></span>
        <svg xmlns="http://www.w3.org/2000/svg" width="24" height="25">
            <path fill-rule="evenodd" fill="#1D7A8C"
                d="M22.93 13.616c.575 0 1.068-.481 1.068-1.122 0-.56-.493-1.041-1.068-1.041-.987 0-9.26-.321-9.26-10.053 0-.921-1.096-1.3-1.671-1.3-.576 0-1.069.379-1.069 1.3 0 9.732-8.39 10.053-9.26 10.053H.985c-.575.08-.985.481-.985 1.041 0 1.056.492 1.041 1.67 1.041.466 0 9.26.321 9.26 10.414 0 .56.493 1.041 1.069 1.041.493.71.986-.401.986-.71 0-10.344 8.876-10.664 9.863-10.664h.082z" />
        </svg>
        <span class="line"></span>
    </div>

    <div class="resetpass">

        <div class="d-flex flex-column">
            <input type="Password" asp-for="Password" class="formdecoration resetpassword" placeholder="New Password">
            <span asp-validation-for="Password"></span>
        </div>
        <div class="d-flex flex-column">
            <input type="Password" asp-for="CPassword" class="formdecoration resetpassword"
                placeholder="Confirm Password">
            <span asp-validation-for="CPassword"></span>
        </div>
        <input type="submit" id="resetpasssubmit" value="Change Password">
    </div>
</form>

@section CustomScripts{
<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.19.3/jquery.validate.min.js"
